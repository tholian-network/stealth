@font-face {
  font-family: 'fontello';
  src: url('../font/fontello.eot?80002349');
  src: url('../font/fontello.eot?80002349#iefix') format('embedded-opentype'),
       url('../font/fontello.woff2?80002349') format('woff2'),
       url('../font/fontello.woff?80002349') format('woff'),
       url('../font/fontello.ttf?80002349') format('truetype'),
       url('../font/fontello.svg?80002349#fontello') format('svg');
  font-weight: normal;
  font-style: normal;
}
/* Chrome hack: SVG is rendered more smooth in Windozze. 100% magic, uncomment if you need it. */
